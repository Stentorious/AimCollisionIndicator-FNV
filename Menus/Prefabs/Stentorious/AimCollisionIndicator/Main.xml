	<height>
		<copy> 14 </copy>
		<mul src="me()" trait="_Scale"/>
	</height>
	<zoom> &scale; </zoom>
	<systemcolor> 0 </systemcolor>
	<red> 255 </red>
	<green> 255 </green>
	<blue> 255 </blue>

